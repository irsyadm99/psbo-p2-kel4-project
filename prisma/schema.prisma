// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  // Attributes
  id        String @id @default(cuid())
  firstName String
  lastName  String
  email     String
  password  String
}

model Admin {
  id       String @id @default(cuid(4))
  userName String
  password String
}
